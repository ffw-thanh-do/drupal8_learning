{#
/**
 * @file people.html.twig
 * Default theme implementation to present People data.
 *
 * This template is used when viewing People pages.
 *
 *
 * Available variables:
 * - content: A list of content items. Use 'content' to print all content, or
 * - attributes: HTML attributes for the container element.
 *
 * @see template_preprocess_people()
 *
 * @ingroup themeable
 */
#}
<div{{ attributes.addClass('people') }}>
  {% if content %}
    {{- content -}}
  {% endif %}
</div>
